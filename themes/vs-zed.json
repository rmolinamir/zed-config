{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "author": "rmolinamir",
  "name": "VSZed",
  "themes": [
    {
      "appearance": "dark",
      "name": "Dark (Visual Studio)",
      "style": {
        "accents": [],
        "background": "#1A1A1A",
        "background.appearance": "opaque",
        "border": "#3C3C3C",
        "panel.background": "#1A1A1A",
        "surface.background": "#1A1A1A",
        "drop_target.background": "#383B3D",
        "editor.active_line_number": "#D4D4D4",

        "editor.background": "#1E1E1E",
        "editor.foreground": "#D4D4D4",
        "editor.gutter.background": "#1E1E1E",

        "elevated_surface.background": "#252526",

        "error": "#F48771",
        "error.background": "#3F1D1D",
        "error.border": "#F48771",

        "hint": "#4FC1FF",
        "hint.background": "#252526",
        "hint.border": "#4FC1FF",

        "info": "#9CDCFE",
        "info.background": "#0B2B40",
        "info.border": "#9CDCFE",

        "players": [],
        "tab_bar.background": "#1A1A1A",
        "scrollbar.track.background": "#1A1A1A",

        "syntax": {
          "attribute": { "color": "#9CDCFE" },
          "boolean": { "color": "#569CD6" },

          "comment": { "color": "#6A9955" },
          "comment.doc": { "color": "#6A9955" },

          "constant": { "color": "#4FC1FF" },
          "constructor": { "color": "#4EC9B0" },

          "embedded": { "color": "#D4D4D4" },

          "emphasis.strong": { "color": "#569CD6", "font_weight": 700 },

          "entity.name": { "color": "#C586C0" },
          "function": { "color": "#DCDCAA" },

          "import": { "color": "#C586C0" },

          "keyword": { "color": "#569CD6" },
          "keyword.control": { "color": "#C586C0" },
          "keyword.definition": { "color": "#C586C0" },
          "keyword.directive": { "color": "#C586C0" },
          "keyword.jsdoc": { "color": "#C586C0" },
          "keyword.operator": { "color": "#C586C0" },

          "number": { "color": "#B5CEA8" },

          "operator": { "color": "#D4D4D4" },

          "preproc": { "color": "#C586C0" },

          "property": { "color": "#9CDCFE" },

          "punctuation": { "color": "#D4D4D4" },

          "punctuation.accessor": { "color": "#C586C0" },
          "punctuation.bracket": { "color": "#D4D4D4" },
          "punctuation.delimiter": { "color": "#D4D4D4" },
          "punctuation.list_marker": { "color": "#D4D4D4" },
          "punctuation.special": { "color": "#D4D4D4" },

          "string": { "color": "#CE9178" },
          "string.escape": { "color": "#CE9178" },
          "string.regex": { "color": "#CE9178" },
          "string.special": { "color": "#CE9178" },
          "string.special.symbol": { "color": "#CE9178" },

          "tag": { "color": "#569CD6" },

          "text.literal": { "color": "#CE9178" },

          "type": { "color": "#4EC9B0" },

          "variable": { "color": "#9CDCFE" },
          "variable.special": { "color": "#9CDCFE" },
        },

        "text": "#D4D4D4",
        "text.disabled": "#6D6D6D",
        "text.muted": "#969696",
        "text.placeholder": "#808080",
        "toolbar.background": "#1A1A1A",

        "warning": "#D7BA7D",
        "warning.background": "#3B2D00",
        "warning.border": "#D7BA7D",
      },
    },
  ],
}
